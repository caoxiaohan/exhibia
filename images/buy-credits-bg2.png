			$("#connection-strength-bg").css('background','none');
			$("#connection-tooltip-content").html("<b>Connection Lost!</b><br>Try Refreshing Page");
		}
		else {
			var currentTime = new Date();
			// check if the javascript has been run recently, if not resync
			if((currentTime.getTime() - window.sendTime.getTime()) > 2000) {
				window.cleanupTimers = true;
			}
			else {
				displayTimers();
			}
		}
		window.missedResponseCount++;
try {
		window.sendTime = new Date();
		$.manageAjax.add('timerAjaxManager', {
		dataType: "json",
		timeout: 1900,
		success: function(data) {
			window.receiveTime = new Date();
			var fDiff = (window.receiveTime.getTime() - window.sendTime.getTime());
			if(window.hasControlBar) {
				if(fDiff < 1000) {
					if(window.missedResponseCount > 1) {
