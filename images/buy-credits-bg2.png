			//fDiff = fDiff/100;
			//var nQuality = Math.round(200/((fDiff)*(fDiff)));
			//if(nQuality > 100) nQuality = 100;

			if(window.cleanupTimers) {
				window.cleanupTimers = false;
				var objCurrentItems = new Object();
