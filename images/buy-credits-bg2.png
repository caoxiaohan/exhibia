// A little helper function, added onto $. that counts
// items in an "associative array" (which doesn't exist
// in JS)
jQuery.extend( {
    objSize: function(obj) {
    var size = 0, key;
        for (key in obj) {
            if (obj.hasOwnProperty(key)) size++;
        }
        return size;
    }
});

