	function displayTimers()
	{ 
		try {
			for( var iKey in window.objAuctionTimes ) {
				var aryTime = window.objAuctionTimes[iKey];

				var strHours = aryTime['th'];
				var nHours = Number(strHours);
				var strMins = aryTime['tm'];
				var nMins = Number(strMins);
				var strSecs = aryTime['ts'];
				var nSecs = Number(strSecs);

				if(nHours > 0 || nMins > 0) {
					if(nSecs == 0) {
						if(nMins > 0) {
							nMins--;
						}
						else if(nHours > 0) {
							nHours--;
							nMins = 59;
						}
						nSecs = 59;
					}
					else {
						nSecs--; 
					}
				}
				else if(nSecs > 0) {
					nSecs--; 
				}
                else { // if the timer stays at 1 for more than 2 seconds, see if the item ended
                    if(typeof window.objEndedItems[iKey] == "number") {
                        if(window.objEndedItems[iKey] == 0) {
                            if(window.missedResponseCount < 2) {
                                //Get ended auctions via ajax
                                $.post('../auctions/get-expired-timer-info/auction_id_list/'+iKey, { }, function(auction_data){

                                    $.each(auction_data, function(i,item){
                                        updateEnded(item);
                                    });

                                }, "json");
                                delete window.objEndedItems[iKey];
                                delete window.objAuctionTimes[iKey];
                            }
                        }
                        else {
                            window.objEndedItems[iKey]--;
                        }
                    }
                    else {
                        window.objEndedItems[iKey] = Number(2);
                    }
                }

				strHours = ""; strMins = ""; strSecs = "";
				if(nHours < 10)
					strHours = "0";
				strHours += String(nHours);
				if(nMins < 10)
					strMins = "0";
				strMins += String(nMins);
				if(nSecs < 10)
					strSecs = "0";
				strSecs += String(nSecs);

                var objTime = new Array();
                objTime['th'] = strHours; objTime['tm'] = strMins; objTime['ts'] = strSecs; objTime['locked'] = false;

                window.objAuctionTimes[iKey] = objTime;

				updateTimer(iKey,strHours,strMins,strSecs);
			}
		} catch(e) { }
	}

	function updateTimer(iKey,strHours,strMins,strSecs)
	{ 
		try {
			//var chrExpiring = ""; // using this for performance reasons
            var blnMark = false;
            var blnProcessing = false;

			//var objItemTime = $('span.time'+iKey);

			if(strHours == "00" && strMins == "00") {
                var nSecs = Number(strSecs);
                
				if(nSecs <= 10) {
                    /*
					var cr = 250; cg = 130; cb = 30;
					cr = cr - (4*(10 - nSecs));
					var cg = cg - (13*(10 - nSecs));
					var cb = cb - (3*(10 - nSecs));
                    */
					//$('.time'+iKey).addClass("time-expiring");
					//objItemTime.css("color", "rgb("+cr+","+cg+","+cb+")");
					//chrExpiring = "a";

                    blnMark = true;

					if(nSecs == 0) { // item is still processing
						t_showProcessing(iKey);
                        blnProcessing = true;
					}
				}
				else {
					//$('.time'+iKey).removeClass("time-expiring");
					//objItemTime.css("color","rgb(0,0,0)");
					//chrExpiring = "r";
				}
			}

            if(!blnProcessing) {
                t_hideProcessing(iKey);
            }

//$('#debugdiv').html($('#debugdiv').html()+strSecs+"<br>");
			//objItemTime.html(strHours + ":" + strMins + ":" + strSecs);

			for( var sPrefix in window.aryPrefixes )
			{
				var objTimeH = $('#'+window.aryPrefixes[sPrefix]+'timeh'+iKey);

                if(objTimeH.length > 0) {
                    var objTimeM = $('#'+window.aryPrefixes[sPrefix]+'timem'+iKey);
                    var objTimeS = $('#'+window.aryPrefixes[sPrefix]+'times'+iKey);

                    objTimeH.html(strHours);
                    objTimeM.html(strMins);
                    objTimeS.html(strSecs);
                    if(blnMark) {
                        objTimeS.addClass("mark");
                    }
                    else {
                        objTimeS.removeClass("mark");
                    }
                }
                /*
				if(chrExpiring == "a") {

				}
				else if (chrExpiring == "r") {

				}
                */
			}
		} catch(e) { }
	}

    function bidResetTimer(auction_id,resetSecs) {
        
        if (typeof window.objAuctionTimes[auction_id] == "object") {
            delete window.objAuctionTimes[auction_id];
        }

        for( var sPrefix in window.aryPrefixes )
        {
            var objTimeS = $('#'+window.aryPrefixes[sPrefix]+'times'+auction_id);

            if(objTimeS.length > 0) {
                objTimeS.removeClass("mark");
                var strSecs = objTimeS.html();
                var nSecs = Number(strSecs);
                var nResetSecs = Number(resetSecs);
                if(nSecs < nResetSecs) {
                    var strResetSecs = "";
                    if(nResetSecs < 10) strResetSecs = "0";
                    strResetSecs += String(nResetSecs);
                    objTimeS.html(strResetSecs);
                }
            }
        }
    }

    function updateUserInfo() {
		try {
			$.post('../users/get-periodic', { }, function(data){
				//Display accurate credits
				setCredits(data['c']);
			}, "json");
		}
		catch(e) { }
    }

    function updateEnded(item)
    {
        updateAvatar(item.id,item.a,item.h);

        var strState = 'buy';
        
        t_updateButton(item.id,strState);
        t_showEnded(item.id);
        s_hideBuyNow(item.id);

        if(window.hasDetailWinnerSavings) {
            gDetail_loadWinnerSavings();
        }

    }

    function idleHandler() {
        window.idle_seconds = 0;
		window.isIdle = true;
		window.clearTimeout(window.idleTimeout);
		window.clearInterval(window.t);
		var strHTML = '<div id="idle-confirm">Still Here?<br><br>You have been idle for more than '+window.idleMinutes+' minutes. Please click Resume to continue.<br></div>';
		$('body').append(strHTML);
		$('#idle-confirm').dialog({
			width: 400,
			minWidth: 400,
			maxWidth: 600,
			minHeight: 200,
			maxHeight: 400,
			modal: true, 
			title: 'Idle Confirmation',
			buttons: {
				"Resume": function() { 
					$(this).dialog('destroy');
					$(this).remove();
					window.isIdle = false;
					timedCount();
					window.t = window.setInterval("timedCount()",1000);
					window.idleTimeout = window.setTimeout("idleHandler()", window.idleMinutes * 60000);
				}
			},
			beforeclose: function(){ 
				$(this).dialog('destroy');
				$(this).remove();
				window.isIdle = false;
				timedCount();
				window.t = window.setInterval("timedCount()",1000);
				window.idleTimeout = window.setTimeout("idleHandler()", window.idleMinutes * 60000);
			}
		});
	}
</script>

