function buy(product_id,auction_id,obj)
{
    if(typeof auction_id == "undefined") {
        auction_id = 0;
    }
    //override product if a different one has been selected
    if(auction_id > 0) {
        var objProductId = $('#auction-product'+auction_id);
        if(objProductId.length > 0) {
            var nProductId = Number(objProductId.html());
            if(nProductId > 0) {
                product_id = Number(objProductId.html());
            }
        }
    }

    window.location = '../account/payment/id/'+product_id+'../aid/'+auction_id;
}

function getNextProduct(auction_id,sort_order,max_index,direction) {

	var sAuctionId = String("a"+auction_id);

    if(window.aryCurrentItemIndexes[sAuctionId] === undefined) {
        var nextIndex = sort_order;
    }
    else {
        var nextIndex = window.aryCurrentItemIndexes[sAuctionId];
    }

    if(direction != null && direction == "prev") {
        if(nextIndex > 0) {
            nextIndex--;
            _showArrow(auction_id,"next",true);
        }
        if(nextIndex == 0) {
            _showArrow(auction_id,direction,false);
        }
    }
    else {
        if(nextIndex < max_index) {
            nextIndex++;
            _showArrow(auction_id,"prev",true);
        }
        if(nextIndex == max_index) {
            _showArrow(auction_id,direction,false);
        }
    }

    if(window.aryNextItems[sAuctionId] === undefined || window.aryNextItems[sAuctionId]['i'+nextIndex] === undefined) {
        //This puts the apropriate content in window.aryNextItems[auction_id][nextIndex]
		if(window.aryNextItems[sAuctionId] === undefined) window.aryNextItems[sAuctionId] = new Array();
        $('#next-items-container').load('../global/next-items/auction_id/'+auction_id+'../sort_order/'+sort_order+'../direction/'+direction, function() {
            window.aryCurrentItemIndexes[sAuctionId] = nextIndex;
            _displayCurrentIndexProduct(auction_id,nextIndex);
        });
    }
    else {
        window.aryCurrentItemIndexes[sAuctionId] = nextIndex;
        _displayCurrentIndexProduct(auction_id,nextIndex);
    }
}

function _displayCurrentIndexProduct(auction_id,currentIndex) {

    var sAuctionId = String("a"+auction_id);
    var currentIndex = String("i"+currentIndex);

    if(window.aryNextItems[sAuctionId] != undefined && window.aryNextItems[sAuctionId][currentIndex] != undefined) {

        if(window.aryNextItems[sAuctionId][currentIndex]['id'] != undefined) {
            // window.aryNextItems: id,product_name,etilize_id,image,product_price,product_msrp

            var aryItem = window.aryNextItems[sAuctionId][currentIndex];
            var objTitle = $('#preview-item-title'+auction_id);
            var strObjImage = 'preview-item-image'+auction_id;
            var objImage = $('#'+strObjImage);
            var objPrice = $('#preview-item-price'+auction_id);
            var objMSRP = $('#preview-item-msrp'+auction_id);
            var objProductId = $('#auction-product'+auction_id);
            if(objTitle.length > 0) {
                objTitle.html(aryItem['product_name']);
            }
            if(objImage.length > 0) {
                objImage.hide().html(aryItem['image']);
                window.setTimeout("_fadeIn('"+strObjImage+"')",250);
            }
            if(objPrice.length > 0) {
                objPrice.html(aryItem['product_price']);
            }
            if(objMSRP.length > 0) {
                objMSRP.html(aryItem['product_msrp']);
            }
            if(objProductId.length > 0) {
                objProductId.html(aryItem['id']);
            }
        }
    }
}

function _fadeIn(strObjId) {
    var objImage = $('#'+strObjId);
    objImage.fadeIn("slow");
}

function _showArrow(auction_id,direction,bShow) {
    if(direction == 'prev') {
        var objArrow = $('#photo-prev'+auction_id);
    }
    else {
        var objArrow = $('#photo-next'+auction_id);
    }
    if(objArrow.length > 0) {
        if(bShow) {
            objArrow.css("visibility", "visible");
        }
        else {
            //objArrow.hide();
            objArrow.css("visibility", "hidden");
        }
    }
}

function watchItem(auction_id,product_id,strID) {

    var blnChanged = false;

    var objWatch = $('#'+strID);

    if(objWatch.length == 0) return;

    if(objWatch.hasClass('watching')) {
        var strMode = 'remove';
    }
    else {
        var strMode = 'add';
    }

    $.post('../global/watch/auction_id/'+auction_id+'../product_id/'+product_id+'../mode/'+strMode, { }, function(watch){

        if(watch['success'] == "y")
        {
            objWatch.unbind(); // remove tooltips
            var objWatchCount = $('#watch-count');
            var objWatchCount_s = $('#watch-count_s');
            var nWatchCount = 0;

            if(strMode == 'add') {
                objWatch.addClass('watching');
                if(objWatchCount.length > 0) {
                    nWatchCount = Number(objWatchCount.html());
                    objWatchCount.html(nWatchCount + 1);
                }
            }
            else {
                objWatch.removeClass('watching');
                if(objWatchCount.length > 0) {
                    nWatchCount = Number(objWatchCount.html());
                    if(nWatchCount > 0) {
                        objWatchCount.html(nWatchCount - 1);
                    }
                }
            }
            if(objWatchCount_s.length > 0) {
                if(nWatchCount == 1) {
                    objWatchCount_s.html('');
                }
                else {
                    objWatchCount_s.html('s');
                }
            }
            // Reload watches content
            reloadWatch();
        }
        else if(watch['success'] == "wrong-mode") {
            if(strMode == 'add') {
                objWatch.addClass('watching');
            }
            else {
                objWatch.removeClass('watching');
            }
        }
    }, "json");

}

function reloadWatch() {
    var objWatchList = $('#watch-list-content');
    if(objWatchList.length > 0) {
        objWatchList.load('../global/watch-items/count/20/no-layout/1');
    }
}

