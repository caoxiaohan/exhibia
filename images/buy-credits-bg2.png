function t_getPrice(item_id) {
    var maxPrice = 0;
    var price = 0;

    for( var key in window.aryPrefixes ) {
        objPrice = $('#'+window.aryPrefixes[key]+'price'+item_id);
        if(objPrice.length > 0) {
            price = parseFloat(objPrice.html());
            if(price > maxPrice) {
                maxPrice = price;
            }
            else {
                price = maxPrice;
            }
        }
    }

    return price;
}

function updatePrice(item_id,price,aryPrefix)	{

    if(!aryPrefix) var aryPrefix = window.aryPrefixes;

    for( var key in aryPrefix ) {

        var objPrice = $('#'+aryPrefix[key]+'price'+item_id);
        if(objPrice.length == 0) continue;

        price = Number(price);
        objPrice.html(price.toFixed(2));

        var strStart = objPrice.css('fontSize');
        var nStart = Number(strStart.substring(0,2));

        // The following is a safety. Numbers were staying at a larger font if the animation failed
        // As a result, this function can currently only handle font sizes less than 16, 16, and 30.
        if(nStart >= 30) {
            strStart = '30px';
            nStart = 30;
        }
        else if(nStart > 16) {
            strStart = '16px';
            nStart = 16;
        }

        var nEnd = nStart + 4;
        var strEnd = String(nEnd) + 'px';

        var strObject = '#'+aryPrefix[key]+'price'+item_id;

        _updatePriceHelper(strObject,strStart,strEnd);
    }
}

function _updatePriceHelper(strObject,strStart,strEnd) {

    $(strObject).stop().css({ fontSize:strStart }).animate({ fontSize:strEnd }, 100, "linear", function(){
        try {
            $(strObject).css({ fontSize:strEnd }).animate({ fontSize:strStart }, 150, "linear", function(){
                // Animation complete, clear safety timeout
                //clearTimeout(flashCheckID);
                //alert("cleared: " + flashCheckID);
            });
        }
        catch(e1) { }
    });
}

