		function showBonusCredits(nBonusCredits) {
			var objBonusNum = $('#bonus-credit-num');
			if(objBonusNum.length == 0) { return; }

			objBonusNum.html(String(nBonusCredits));

			window.clearTimeout(window.bonusCreditTimeout);
			window.bonusCreditTimeout = setTimeout('hideBonusCredits()',5000);

			//Add and animate bonus credit images
			window.aryBonusCredits = new Array();
			var strHTML = '';
			for(var i=0;i<nBonusCredits;i++) {
				if(i>=10) {
					break;
				}
				var imgID = "bonus-credit-image"+i;
				window.aryBonusCredits.push('#'+imgID);
				strHTML += '<img id="'+imgID+'" src="wavee-credit.gif" alt=""/>';
			}
			var objBonusImages = $('#bonus-credit-images');
			objBonusImages.html(strHTML);
			for( var index in window.aryBonusCredits ) {
				var objImage = $(window.aryBonusCredits[index]);
				if(objImage.length > 0) {
					var x = getRandInt(110)+150;
					var y = getRandInt(50)+65;
					objImage.css('margin',y+'px 0 0 '+x+'px');
				}
			}
			
			$('#bonus-credit-images').stop().css({ opacity:0 }).fadeTo(1200, 0.8, function(){
				//Display Info Box
				$('#bonus-credit-info').stop().css({ opacity:0 }).show().addClass('animating').fadeTo(400, 0.9, function(){
					$('#bonus-credit-info').fadeTo(300, 0.1, function(){
						$('#bonus-credit-info').removeClass('animating').fadeTo(500,1, function(){
							for( var index in window.aryBonusCredits ) {
								var objImage = $(window.aryBonusCredits[index]);
								if(objImage.length > 0) {
									objImage.animate({ marginTop:'-28px', marginLeft:'-5px', width:'16px', height:'16px' }, 1000, "linear", function(){
										window.clearTimeout(window.bonusCreditTimeout);
										window.bonusCreditTimeout = window.setTimeout('finishBonusAnimation('+nBonusCredits+')',300);
									});
								}
							}
						});
					});
				});
			});
		}

		function finishBonusAnimation(nBonusCredits) {
			subtractCredits(-1*nBonusCredits,true);
			hideBonusCredits();
		}

		function hideBonusCredits() {
			$('#bonus-credit-info').fadeOut(1200);
			$('#bonus-credit-images').fadeOut(400);
		}

		function alertBidFailed(strHTML)
		{
        $('body').append(strHTML);
        $('#bid-failed-alert').dialog({
            width: 400,
			minWidth: 400,
			maxWidth: 600,
			minHeight: 200,
			maxHeight: 400,
                        resizable: false,
			modal: true,
			title: 'Bid Failed Alert!',
            buttons: {
                "Close": function() {
                    $(this).dialog('destory');
                    $(this).remove();
                }
            },
            beforeClose: function(){
                $(this).dialog('destroy');
                $(this).remove();
            }
            });
		}
</script>
