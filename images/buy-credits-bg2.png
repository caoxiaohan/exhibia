function replaceEndedAuctions() {
    itemPrefix = 'flash-bg';

    endAuc = new Object;
    endAuc = window.aryEndedAuctions;
    objSize = $.objSize(endAuc);
    if(objSize==0){return true;}

    $.manageAjax.create('auctionReplaceQueue',
            { queue: "true",
              contentType: 'html',
              maxRequests: 2,
              abortOld: false,
              preventDoubbleRequests: true,
              cacheResponse: false
        });
    //alert('window.aryEndedAuctions string' + window.aryEndedAuctions.toString());
    //console.log('Going thru ended auctions:');
    //console.log('-------------------------------');
    for(aucid in endAuc) {
        //console.log('Auc: '+aucid);
        //alert('aucid, before manageAjax.add, is ' + aucid);
        //alert('Going to call ' + endAuc[aucid].controller + ' to replace ' + aucid + ' in div ' + endAuc[aucid].updateDiv);
        if( ($('#btn-bid'+aucid).length != 0) &&  $('#btn-bid'+aucid).hasClass('pay') ) {
            for(idIndex in window.aryEndedAuctions) {
                if(idIndex == aucid) {
                    delete window.aryEndedAuctions[idIndex];
                    break;
                }
            }
            continue;
        }
/*
        regTime = $('#'+itemPrefix+aucid).data("regTime");
        if( regTime == null || regTime == 'null' || regTime == '' || regTime == ' ' ) {
            tstamp = new Date().getTime();
            $('#'+itemPrefix+aucid).data('regTime', tstamp);
            continue;
        } else {
            timeAgo = Math.floor(parseInt(( (new Date().getTime()) - parseInt(regTime)) / 1000));
            if( parseInt(timeAgo) <= parseInt(aucReplaceDelay) ) {
                continue;
            }
        }
  */
        $.manageAjax.add('auctionReplaceQueue', {
            //success: function(html) {
            success: function(html) {
              if(typeof(endAuc[''+aucid+'']) == 'undefined') { return true;}

              runningAuctions = new Array();

              // Grab every auction ID in the displayed page.
              for(divid in aryDivControllers) {
                  if( (divid != endAuc[''+aucid+''].updateDiv) && ($('#'+divid).length != 0) ) {
                      $('#'+ divid +' div[id^="'+ itemPrefix +'"]').each(function(index) {
                         runningAuctions.push($(this).attr('id'));
                      });
                  } /* else {
                      $('#'+ endAuc[''+aucid+''].updateDiv +' div[id^="'+ itemPrefix +'"]').each(function(index) {
                         runningAuctions.push($(this).attr('id'));
                      });
                 } */
              }
              // Turn those IDs into a string...
              runAucString = runningAuctions.toString();
              //console.log('Auctions running on the page: ' + runAucString);

              //alert('running auction string is ' + runAucString);
              
              numNewAucs = 0;
              searchReturn = 0;
              ajaxAucs = new Array();
              for(divid in aryDivControllers) {
                // First see if the div ID associated with the controller exists
                // in the returned ajax.
                if($('#'+divid, html).length != 0) {
                  $('#'+ divid +' div[id^="'+ itemPrefix +'"]', html).each(function(index) {
                    searchReturn = runAucString.search($(this).attr('id'));
                    // If one of the IDs returned in the ajaxHTML is not on the page...
                    if(searchReturn == -1) {
                        ajaxAucs.push($(this).attr('id'));
                        numNewAucs++;
                    }
                  });
                // If it's not there then look for auctions in whatever it is
                // that the ajax returned.
                } /* else {
                   $('ul div[id^="'+ itemPrefix +'"]', html).each(function(index) {
                    searchReturn = runAucString.search($(this).attr('id'));
                    // If one of the IDs returned in the ajaxHTML is not on the page...
                    if(searchReturn == -1) {
                        ajaxAucs.push($(this).attr('id'));
                        numNewAucs++;
                    }
                  });
                } */
              }

              // If the controller for the div returned no new auctions then
              // get out of here.
              if(numNewAucs < 1) {
                return false;
              }

              // Fade out the ended auction.
//              $('#'+itemPrefix+aucid+'').removeData('regTime');
              $('#'+itemPrefix+aucid+'').parent('li.item').animate(
                  { opacity: 'toggle', duration: 2000 },
                  // The function called after the fading out is done.
                  // Here, it's a closure passing in locally scoped
                  // variables so they keep their values.
                  (function(ajaxHTML, newAuctions) {
                      return function() {
                                haveNew = 0;
                                newAucID = '';
                                for(auc in newAuctions) {
                                    if($('#'+newAuctions[auc]+'', ajaxHTML).length != 0) {
                                        haveNew = 1;
                                        newAucID = newAuctions[auc];
                                        break;
                                    }
                                }

                                if(haveNew) {
                                    // Fade out the ended auction, then replace the contents with the new auction.
                                    $('#'+itemPrefix+aucid+'').parent('li.item').replaceWith('<li class="item">' + $('#'+newAucID, ajaxHTML).parent('li.item').html() + '</li>').animate({ opacity: 'toggle', duration: 'slow' });
                                    // Grab the height of the new auction
                                    newAucHeight = $('#'+newAucID).css('height');
                                    // Immediately set it's height to zero
                                    $('#'+newAucID).css('height', 0);
                                    // Animate the height back to the original height
                                    $('#'+newAucID).animate({'height': newAucHeight, duration: 1500});

                                    // Make damn sure the ended auction ID is out of the aryAuctions array.
                                    for(idIndex in window.gTimer_aryAuctions) {
                                        if(window.gTimer_aryAuctions[idIndex] == aucid) {
                                            retVal = window.gTimer_aryAuctions.splice(idIndex, 1);
                                            break;
                                        }
                                    }

                                    // Make damn sure the ended auction ID is out of the endedAuctions array.
                                    for(idIndex in window.aryEndedAuctions) {
                                        if(idIndex == aucid) {
                                            delete window.aryEndedAuctions[idIndex];

                                            break;
                                        }
                                    }

                                    // Push the new auction ID onto the stack.
                                    window.gTimer_aryAuctions.push(newAucID.replace(itemPrefix, ''));
                                    gTimer_setAuctions();
                                }
                      }
                  })(html, ajaxAucs));
              },
          url: endAuc[aucid].controller
      });
    }
    //console.log('-------------------------------');
    //console.log('');
}



//state: normal, processing, winning
function t_updateButton(item_id,state) {

    if(state == 'outbid') state = 'normal';

    var objWinningStar = $('#winning-star'+item_id);
    if(objWinningStar.length > 0) {
        if(state == 'winning') {
            objWinningStar.addClass('star-on');
            var objCredits = $('#credits-per-bid'+item_id);
            if(objCredits.length > 0) {
                objCredits.addClass('hidden');
            }
        }
        else {
            objWinningStar.removeClass('star-on');
            var objCredits = $('#credits-per-bid'+item_id);
            if(objCredits.length > 0) {
                objCredits.removeClass('hidden');
            }
        }
    }

    var objButton = $('#btn-bid-large'+item_id);
    if(objButton.length > 0 && !objButton.hasClass(state)) {

        updateButtonObj(objButton,state,large=true);
        objButton.effect('bounce');
    }

    for( var key in window.aryPrefixes ) {
        var strObjID = '#'+window.aryPrefixes[key]+'btn-bid'+item_id;
        objButton = $(strObjID);
        if(objButton.length == 0 || objButton.hasClass(state)) continue;

        updateButtonObj(objButton,state);
        objButton.effect('bounce');
    }
}

function updateButtonObj(objButton,state) {
    productType = $('#product-type').text();
    if( (productType == 'c' && state == 'winning') || (productType == 'c' && state == 'processing') || (productType == 'c' && state == 'pay') || (productType != 'c')) {
        objButton.removeClass().addClass('btn ' + state);
    } else {
        objButton.removeClass().addClass('hidden');
    }

}

function t_showEnded(item_id) {
    for( var sPrefix in window.aryPrefixes ) {
        var strObjID = '#'+window.aryPrefixes[sPrefix]+'ended'+item_id;
        var objEnded = $(strObjID);
        if(objEnded.length > 0) {
            objEnded.show();
        }
    }
    objEnded = $('#btn-bid-large'+item_id);
    if(objEnded.length > 0) {
