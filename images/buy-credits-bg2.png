function updateDetailSavings(auction_id,chrCreditType,fCreditValue,fProductPrice,sB2B)
{
	if($('#savings-msrp'+auction_id).length == 0 || chrCreditType == "")
	{
		return;
	}

	if(sB2B != 'y') {
		var objB2BLineInfo = $('#b2b-line-info'+auction_id);
		if(objB2BLineInfo.length > 0) {
			objB2BLineInfo.removeClass('info-box-on');
			$('#btn-buy-now-large').addClass('hidden').hide();
            if($("#b2b-off-overlay").length > 0) {
                showB2BOverlay();
            }
		}
	}

	fCreditValue = getNumber(fCreditValue);
	fProductPrice = getNumber(fProductPrice);

	$('#savings-container .savings-price'+auction_id).html(fProductPrice.toFixed(2));

	if(chrCreditType == "f") {
		var nNewCount = getNumber($('#savings-container .savings-free-count'+auction_id).html()) + 1;
		if(nNewCount > 0) {
			$('#savings-container .savings-free-count'+auction_id).html(nNewCount);
			$('#savings-container .savings-buy-free-count'+auction_id).html(nNewCount);
		}
	}
	else if(chrCreditType == "p") {
		var nNewCount = getNumber($('#savings-container .savings-paid-count'+auction_id).html()) + 1;
		if(nNewCount > 0) {
            if(nNewCount == 100) {

            }
			$('#savings-container .savings-paid-count'+auction_id).html(nNewCount);
			$('#savings-container .savings-buy-paid-count'+auction_id).html(nNewCount);
		}
		var fNewValue = getNumber($('#savings-container .savings-paid-value'+auction_id).html());
		fNewValue = fNewValue + fCreditValue;
		if(fCreditValue > 0 && fNewValue > 0) {
			$('#savings-container .savings-paid-value'+auction_id).html(fNewValue.toFixed(2));
			if(sB2B == 'y') {
				$('#savings-container .savings-buy-paid-value'+auction_id).html(fNewValue.toFixed(2));
			}
		}
	}
	else if(chrCreditType == "b") {
		var nNewCount = getNumber($('#savings-bonus-count'+auction_id).html()) + 1;
		if(nNewCount > 0) {
			$('#savings-bonus-count'+auction_id).html(nNewCount);
		}
		if(sB2B == 'y' && $('#savings-container .savings-buy-bonus-count').length > 0) {
            $('#savings-container .savings-buy-bonus-count').html(nNewCount);
			var fNewValue = getNumber($('#savings-container .savings-buy-bonus-value'+auction_id).html());
			fNewValue = fNewValue + fCreditValue;
			if(fCreditValue > 0 && fNewValue > 0) {
				$('#savings-container .savings-buy-bonus-value'+auction_id).html(fNewValue.toFixed(2));
			}
		}
	}

    if(chrCreditType == "p" || chrCreditType == "b") {
        addHalo();
    }

	var msrp = getNumber($('#savings-container .savings-msrp'+auction_id).html());
	var fNewAmount = msrp;
	fNewAmount = fNewAmount - fProductPrice - getNumber($('#savings-container .savings-paid-value'+auction_id).html());
	if(fNewAmount < 0) {
		fNewAmount = 0;
	}
	$('#savings-container .savings-total'+auction_id).html(fNewAmount.toFixed(2));

	var fNewPercentage = msrp;
	if(fNewPercentage <= 0) {
		fNewPercentage = 0;
	}
	else {
		fNewPercentage = getNumber(100 * fNewAmount / fNewPercentage);
	}
	$('#savings-container .savings-total-percent'+auction_id).html(fNewPercentage.toFixed(0));

	if(sB2B == 'y' && fCreditValue > 0) {
		fNewAmount = getNumber($('#savings-buy-total'+auction_id).html());
		fNewAmount = fNewAmount - fCreditValue;
		if(fNewAmount < 0) {
			fNewAmount = 0;
		}
		$('#savings-container .savings-buy-total'+auction_id).html(fNewAmount.toFixed(2));
		//Update the one by the buy button if exists
		var objBuyPrice = $('#buy-now-price'+auction_id);
		if(objBuyPrice.length > 0) {
			objBuyPrice.html(fNewAmount.toFixed(2));
		}
	}
}

function buildGlobalTooltip(ttWidth,ttHeight,strTip,strElementId) {
    var blnChanged = false;

    if(ttWidth == 0) {
        var ttWidth = 100;
        var ttHeight = 0;
    }
    var objTooltip = $('#global-tooltip');
    if(objTooltip.length > 0) {
        var cWidth = ttWidth - 16;
        if(ttHeight > 24) {
            var cHeight = ttHeight - 24;
            objTooltip.width(ttWidth).height(ttHeight);
            $('#global-tooltip .tooltip-left').height(cHeight);
            $('#global-tooltip .tooltip-center').height(cHeight);
            $('#global-tooltip .tooltip-right').height(cHeight);
        }
        $('#global-tooltip .tooltip-center').width(cWidth);

        if(strTip == "" && strElementId != "") {
            var objElement = $("#"+strElementId);
            if(objElement.length > 0) {
                if(objElement.html() != $('#global-tooltip-content').html()) {
                    $('#global-tooltip-content').html(objElement.html());
                    blnChanged = true;
                }
            }
        }
        else {
            if(strTip != $('#global-tooltip-content').html()) {
                $('#global-tooltip-content').html(strTip);
                blnChanged = true;
            }
        }
    }
    return blnChanged;
}

function hideGlobalTooltip() {
    var objTooltip = $('#global-tooltip');
    if(objTooltip.length > 0) {
        objTooltip.hide();
    }
}

function fadeGlobalTooltip() {
    var objTooltip = $('#global-tooltip');
    if(objTooltip.length > 0) {
        objTooltip.fadeOut('slow');
    }
}

